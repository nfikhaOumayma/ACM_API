IF OBJECT_ID('DBO.ACM_HABILITATION_IHM_BUTTON', 'U') IS NOT NULL
	DROP TABLE DBO.ACM_HABILITATION_IHM_BUTTON;

-------------------
----- TABLE : ACM_HABILITATION_IHM_BUTTON 
CREATE TABLE ACM_HABILITATION_IHM_BUTTON(   
	ID_ACM_HABILITATION_IHM_BUTTON BIGINT NOT NULL IDENTITY(1,1) PRIMARY KEY,
	CLIENT VARCHAR (256) NOT NULL,
	CODE_IHM_BUTTON  VARCHAR (512) NOT NULL,
	IHM_BUTTON VARCHAR (512) NOT NULL,
	DESCRIPTION VARCHAR (512) NULL,
    ID_ACM_HABILITATION_IHM_ROUTE BIGINT NOT NULL,
			
	ACM_ENABLED BIT NOT NULL,
	DATE_INSERTION DATE NULL,
	INSERT_BY  VARCHAR (256) NULL,
	DATE_LAST_UPDATE DATE NULL,
	UPDATED_BY  VARCHAR (256) NULL,
	ACM_VERSION INT NULL,
	
	FOREIGN KEY (ID_ACM_HABILITATION_IHM_ROUTE) REFERENCES ACM_HABILITATION_IHM_ROUTE (ID_ACM_HABILITATION_IHM_ROUTE)
);