	<queryString>
		<![CDATA[SELECT 
	CUAccount.CUAccountID
	,CULoan.CULoanID
	,CUAccount.AccountNumber
	,Branches.BranchID
	,Branches.Description Branch_Description
	,Branches.Code Branch_Code
	,(SELECT UserDefinedFieldLinks.Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldLinks.BranchID=Branches.BranchID AND UserDefinedFieldLinks.UserDefinedFieldID=63) Branch_Manager
	,REPLACE(Customer.Name,',','') Customer_Name
	,CASE 
		WHEN ((SELECT UserDefinedFieldLinks.Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldLinks.CustomerID=Customer.CustomerID AND UserDefinedFieldLinks.UserDefinedFieldID=2) = 1) 
			THEN (SELECT UserDefinedFieldLinks.Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldLinks.CustomerID=Customer.CustomerID AND UserDefinedFieldLinks.UserDefinedFieldID=3)
			ELSE (SELECT UserDefinedFieldLinks.Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldLinks.CustomerID=Customer.CustomerID AND UserDefinedFieldLinks.UserDefinedFieldID=4)
		END National_Resident_ID
	,AH.TownCity HomeCity
	,AH.Address1 HomeStreet
	,AH.Address2 HomeBuildingNumber
	,AH.Address3 HomeUnitNumber
	,AH.PostalCode HomePostalCode
	,Telephone1 Work_Phone_Number
	,Telephone2 Home_Phone_Number
	,Telephone3 MobileNumber
	,email Email
	,ISNULL(AW.TownCity,'') WorkCity
	,ISNULL(AW.Address1,'') WorkStreet
	,ISNULL(AW.Address2,'') WorkBuildingNumber
	,ISNULL(AW.Address3,'') WorkUnitNumber
	,ISNULL(AW.PostalCode,'') WorkPostalCode
	,CULoanPart.LoanAmount 
	,dbo.ConvertNumber2Letters(CULoanPart.LoanAmount, N' ريال سعودي  ') LoanAmountLetter
	,CULoanPart.FlatInterestAmount InterestAmount
	,dbo.ConvertNumber2Letters(CULoanPart.FlatInterestAmount, N' ريال سعودي  ') InterestAmountLetter
	,CULoanPart.IssueFeeAmount
	,dbo.ConvertNumber2Letters(CULoanPart.IssueFeeAmount, N' ريال سعودي  ') IssueFeeAmountLetter
	,CULoanPart.NormalPayment 
	,dbo.ConvertNumber2Letters(CULoanPart.NormalPayment, N' ريال سعودي  ') NormalPaymentLetter
	,CONVERT(DECIMAL(6,4),CULoanPart.APR) APR
	,CULoanPart.TermPeriodNum
	,FORMAT(CULoanPart.InitialPaymentDate,'yyyy/MM/dd') InitialPaymentDate
	,FORMAT(CULoan.FinalPartDate,'yyyy/MM/dd') FinalPaymentDate
	,(SELECT Address1 FROM Company) CompanyAddress
	,(SELECT Address2 FROM Company) CompanyPostalCode
	,(SELECT Address3 FROM Company) CompanyZIPCode
	,(SELECT Telephone1 FROM Company) CompanyTelephone
	,FORMAT(GETDATE(),'yyyy/MM/dd') ExecuteDate_Georgian
	,FORMAT(GETDATE(),'yyyy/MM/dd','ar') ExecuteDate_Hijri
FROM CUAccount
INNER JOIN CULoan ON CULoan.CUAccountID=CUAccount.CUAccountID
INNER JOIN CULoanPart ON CULoanPart.CULoanID=CULoan.CULoanID
INNER JOIN Customer ON Customer.CustomerID=CUAccount.CustomerID
INNER JOIN CustomerPerson ON CustomerPerson.CustomerID=Customer.CustomerID
INNER JOIN Person ON Person.PersonID=CustomerPerson.PersonID
INNER JOIN CustomerAddress CAH ON Customer.CustomerID=CAH.CustomerID AND CAH.AddressTypeID=2
INNER JOIN Address AH ON CAH.AddressID=AH.AddressID
LEFT JOIN CustomerAddress CAW ON Customer.CustomerID=CAW.CustomerID AND CAW.AddressTypeID=1
LEFT JOIN Address AW ON CAW.AddressID=AW.AddressID
INNER JOIN Branches ON CUAccount.BranchID = Branches.BranchID
WHERE CUAccount.ParentAccountID IS NULL AND CUAccount.CUAccountID=$P{ACCOUNTID}]]>
	</queryString>
