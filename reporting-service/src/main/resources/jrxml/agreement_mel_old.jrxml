	<queryString language="SQL">
		<![CDATA[SET LANGUAGE ENGLISH;
SELECT 
[dbo].[ConvertDayNameEnglishToArabic](DATENAME(WEEKDAY, GETDATE())) [DAY_IN_ARABIC],
 DATEPART(DAY, GETDATE()) [DAY],
 [dbo].[ConvertMonthNumberToArabic](DATEPART(MONTH, GETDATE())) [MONTH_OF_THE_YEAR],
 DATEPART(YEAR, GETDATE()) [YEAR],
 (SELECT Description from Branches where Branches.BranchID = CUAccount.BranchID) [BRANCH_NAME],
 (select value from UserDefinedFieldLinks where UserDefinedFieldID = 38 and UserDefinedFieldLinks.BranchID = CUAccount.BranchID) [BRANCH_MANAGER],
 (select Address1 + ' ' + ' '+ TownCity + ' '+ State  from Address,Branches where Address.AddressID = Branches.AddressID and Branches.BranchID = CUAccount.BranchID)  [BRANCH_ADDRESS],
 REPLACE(REPLACE(customer.CorrespondenceName,'Mrs ',''),'Mr ','') [CUSTOMER_NAME],
 FLOOR((CAST (GetDate() AS INTEGER) - CAST(person.DateOfBirth AS INTEGER)) / 365.25) [CUSTOMER_AGE],
 (SELECT Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldID=3 AND UserDefinedFieldLinks.CustomerID=CULoan.CustomerID) [CUSTOMER_NATIONAL_ID],
 ( SELECT UserDefinedFieldListValues.Description FROM UserDefinedFieldLinks, UserDefinedFieldListValues WHERE 
 UserDefinedFieldLinks.Value = CONVERT (nvarchar(1000),UserDefinedFieldListValues.UserDefinedFieldListValueID) AND UserDefinedFieldID=36 AND UserDefinedFieldLinks.CustomerID = CULoan.CustomerID) [CUSTOMER_NATIONAL_ID_ISSUE_PLACE],
 (SELECT Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldID=37 AND CustomerID=CULoan.CustomerID) [CUSTOMER_NATIONAL_ID_ISSUE_DATE],
 ISNULL(( SELECT UserDefinedFieldListValues.Description FROM UserDefinedFieldLinks, UserDefinedFieldListValues WHERE 
 UserDefinedFieldLinks.Value = CONVERT (nvarchar(1000),UserDefinedFieldListValues.UserDefinedFieldListValueID) AND UserDefinedFieldID=26 AND UserDefinedFieldLinks.CustomerID = CULoan.CustomerID),'.................')[CUSTOMER_PROFESSION],
 (SELECT Address1 FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6)  [CUSTOMER_ADDRESS1_STREET],
 (SELECT County FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6)  [CUSTOMER_ADDRESS1_STREET_COUNTY],
 (SELECT TownCity FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6)  [CUSTOMER_ADDRESS1_STREET_COUNTY_TOWNCITY],
 (SELECT State FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6)  [CUSTOMER_ADDRESS1_STREET_COUNTY_STATE],
 (SELECT Address1 FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [CUSTOMER_ADDRESS2_STREET],
 (SELECT County FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [CUSTOMER_ADDRESS2_STREET_COUNTY],
 (SELECT TownCity FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [CUSTOMER_ADDRESS2_STREET_COUNTY_TOWNCITY],
 (SELECT State FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [CUSTOMER_ADDRESS2_STREET_COUNTY_STATE],
 ISNULL(Person.Telephone1, '.................')  [CUSTOMER_MOBILE_NUMBER],
 REPLACE(REPLACE(guarantor.CorrespondenceName,'Mrs ',''),'Mr ','') [GUARANTOR_NAME],
 FLOOR((CAST (GetDate() AS INTEGER) - CAST(Personguarantor.DateOfBirth AS INTEGER)) / 365.25) [GUARANTOR_AGE],
(SELECT Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldID=3 AND CustomerID=guarantor.CustomerID) [GUARANTOR_ID],
(SELECT UserDefinedFieldListValues.Description FROM UserDefinedFieldLinks, UserDefinedFieldListValues WHERE 
 UserDefinedFieldLinks.Value = CONVERT (nvarchar(1000),UserDefinedFieldListValues.UserDefinedFieldListValueID) AND UserDefinedFieldID=36 AND UserDefinedFieldLinks.CustomerID = guarantor.CustomerID) [GUARANTOR_ID_ISSUE_PLACE],
(SELECT Value FROM UserDefinedFieldLinks WHERE UserDefinedFieldID=37 AND CustomerID=guarantor.CustomerID) [GUARANTOR_ID_ISSUE_DATE],
 ISNULL((SELECT UserDefinedFieldListValues.Description FROM UserDefinedFieldLinks, UserDefinedFieldListValues WHERE 
 UserDefinedFieldLinks.Value = CONVERT (nvarchar(1000),UserDefinedFieldListValues.UserDefinedFieldListValueID) AND UserDefinedFieldID=26 AND UserDefinedFieldLinks.CustomerID = guarantor.CustomerID),'.................') [GUARANTOR_PROFESSION],
 (SELECT Address1 FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6) [GUARANTOR_ADDRESS1_STREET],
 (SELECT County FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6)  [GUARANTOR_ADDRESS1_STREET_COUNTY],
 (SELECT TownCity FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6)  [GUARANTOR_ADDRESS1_STREET_COUNTY_TOWNCITY],
 (SELECT State FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 6)  [GUARANTOR_ADDRESS1_STREET_COUNTY_STATE],
 (SELECT Address1 FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [GUARANTOR_ADDRESS2_STREET],
 (SELECT County FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [GUARANTOR_ADDRESS2_STREET_COUNTY],
 (SELECT TownCity FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [GUARANTOR_ADDRESS2_STREET_COUNTY_TOWNCITY],
 (SELECT State FROM CustomerAddress, Address where CustomerAddress.customerID = CULoanGuarantor.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 2)  [GUARANTOR_ADDRESS2_STREET_COUNTY_STATE],
 ISNULL(Personguarantor.Telephone1,'.................')  [GUARANTOR_MOBILE_NUMBER],
 CULoanPart.IssueAmount [ISSUE_AMOUNT_DEGITAL],
 dbo.ConvertNumber2Letters(IssueAmount, N'جنيه') [ISSUE_AMOUNT_LETTER],
 (SELECT Address1 FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 1)  [CUSTOMER_PROJECT_STREET],
 (SELECT County FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 1)  [CUSTOMER_PROJECT_COUNTY],
 (SELECT TownCity FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 1)  [CUSTOMER_PROJECT_STREET_COUNTY_TOWNCITY],
 (SELECT State FROM CustomerAddress, Address where CustomerAddress.customerID = culoan.customerID and CustomerAddress.AddressID = Address.AddressID
 and CustomerAddress.AddressTypeID = 1)  [CUSTOMER_PROJECT_STREET_COUNTY_STATE],
 CULoanPart.TermPeriodNum [TERM_NUM],
 CULoanPart.InitialPaymentDate [FIRST_REPAYMENT_DATE],
 (select MAX(RepaymentDate) from CULoanSchedule where CULoanSchedule.CULoanPartID = CULoanPart.CULoanPartID) [LAST_REPAYMENT_DATE],
 convert(VARCHAR,ROUND(convert(FLOAT,CUProductLoan.FlatRateIntRate),2) ) [FLAT_INTEREST_RATE],
 CAST((select SUM(InterestRepayment) from CULoanSchedule where CULoanSchedule.CULoanPartID = CULoanPart.CULoanPartID) AS INTEGER) [INTEREST],
 CULoanPart.NormalPayment [MONTHLY_REPAYMENT],
 FORMAT(CUProductLoan.PenaltyInterestRate, 'N') + ' % ,' [PENALITY_INTERST_RATE],
 CUAccount.AccountNumber [ACCOUNT_NUMBER],
 CULoanPart.IssueDate [ISSUE_DATE], 
 isnull((SELECT TOP 1 UserDefinedFieldListValues.Description FROM UserDefinedFieldLinks, UserDefinedFieldListValues WHERE 
 UserDefinedFieldLinks.Value = CONVERT (nvarchar(1000),UserDefinedFieldListValues.UserDefinedFieldListValueID) AND UserDefinedFieldID=7 AND UserDefinedFieldLinks.CUAccountID = CUAccount.CUAccountID) , '.................') [IND_CODE_L1],
 isnull((SELECT TOP 1 UserDefinedFieldListValues.Description FROM UserDefinedFieldLinks, UserDefinedFieldListValues WHERE 
 UserDefinedFieldLinks.Value = CONVERT (nvarchar(1000),UserDefinedFieldListValues.UserDefinedFieldListValueID) AND UserDefinedFieldID=8 AND UserDefinedFieldLinks.CUAccountID = CUAccount.CUAccountID),'.................' ) [IND_CODE_L2],
 (select Description from CUProductLoanReasons where CULoan.LoanReasonID = CUProductLoanReasons.LoanReasonID) [LOAN_REASON],
 CAST(CUFee.amount AS INTEGER) AS APPLICATION_FEE,
 CAST((IssueAmount * (select FeePercentage FROM CUFeeBand where CUFeeBand.CUFeeID=CUProductLoan.IssueFeeID))/100 AS INTEGER) AS ADMIN_FEE
 from CULoan 
 INNER JOIN CUAccount ON(CUAccount.CUAccountID=CULoan.CUAccountID) 
 INNER JOIN customer ON(CULoan.CustomerID = customer.CustomerID) 
 INNER JOIN CustomerPerson ON(CustomerPerson.CustomerID = customer.CustomerID) 
 INNER JOIN Person ON(Person.PersonID = CustomerPerson.PersonID) 
 INNER JOIN CULoanPart ON(CULoanPart.CULoanID=CULoan.CULoanID)
 INNER JOIN CUProductLoan ON(CUProductLoan.ProductID=CULoan.ProductID)
 INNER JOIN CUFee ON(CULoan.ProductID = CUFee.cufeeID)
 LEFT JOIN CULoanGuarantor on (CULoanGuarantor.CULoanID = CULoan.CULoanID)
 LEFT JOIN Customer as guarantor ON(guarantor.CustomerID=CULoanGuarantor.CustomerID) 
 LEFT JOIN CustomerPerson as CustomerPersonGuarantor ON(CustomerPersonGuarantor.CustomerID = guarantor.CustomerID) 
 LEFT JOIN Person as Personguarantor ON(Personguarantor.PersonID = CustomerPersonGuarantor.PersonID)
 
where CUAccount.CUAccountID =  $P{ACCOUNTID}]]>
	</queryString>
